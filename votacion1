agma solidity ^0.5.2;

contract voto1{
uint256 CandidatoA;
uint256 CandidatoB;
uint256 CandidatoC;
string ganador;

function VotaA()public {
  CandidatoA++ ; 
}
function VotaB()public {
  CandidatoB++ ; 
}
function VotaC()public {
  CandidatoC++ ; 
}
function resultadosA()view public  returns(uint256){
    return(CandidatoA);
    
}
function resultadosB()view public  returns(uint256){
    return(CandidatoB);
}
function resultadosC()view public  returns(uint256){
    return(CandidatoC);
}
}
